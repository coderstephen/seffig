{
    "name": "switchbox/switchbox",
    "description": "Quick and extensible configuration library that can load and save in JSON, YAML, INI, and more",
    "keywords": ["config", "settings", "configuration", "ini"],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Stephen Coakley",
            "email": "me@stephencoakley.com",
            "homepage": "http://stephencoakley.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/yaml": "~2.5"
    },
    "require-dev": {
        "php-vfs/php-vfs": "*@stable",
        "phpunit/phpunit": "~4.0",
        "sami/sami": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Switchbox\\": "src/",
            "Switchbox\\Tests\\": "tests/"
        }
    }
